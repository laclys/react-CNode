{
  "name": "react-CNode",
  "version": "1.0.0",
  "description": "CNode WebApp",
  "main": "index.js",
  "scripts": {
    "build:web": "webpack --mode development --config build/webpack.config.js",
    "build:server": "webpack --mode development --config build/webpack.config.server.js",
    "dev:web": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.js --mode development",
    "clear": "rimraf dist",
    "build": "npm run clear && npm run build:web && npm run build:server",
    "start": "node server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laclys/react-CNode.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laclys/react-CNode/issues"
  },
  "homepage": "https://github.com/laclys/react-CNode#readme",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.4",
    "html-webpack-plugin": "^3.0.6",
    "react-hot-loader": "^4.0.0",
    "rimraf": "^2.6.2",
    "webpack-dev-server": "^3.1.1"
  }
}
