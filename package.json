    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "mobx": "^4.1.0",
    "mobx-react": "^5.0.0",
    "prop-types": "^15.6.1",
    "query-string": "^6.0.0",
    "react": "^16.2.0",
    "react-async-bootstrapper": "^2.1.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "serialize-javascript": "^1.4.0",
    "serve-favicon": "^2.4.5",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  },
