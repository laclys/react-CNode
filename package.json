  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laclys/react-CNode/issues"
  },
  "homepage": "https://github.com/laclys/react-CNode#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "ejs": "^2.5.7",
